<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sixandone.qixingle.dao.orderDao">

    <insert id="addOrder">
        INSERT INTO `qixingle`.`t_ordermessages` (`OrderNum`, `RentOutOpenID`, `BusinessID`, `BicycleID`, `BicyclePrice`, `CreateTime`, `PayTime`, `EffectTime`, `UnEffectTime`, `OrderStatus`, `PayStatus`)
        VALUES
        (#{OrderNum}, #{RentOutOpenID}, #{BusinessID}, #{BicycleID}, #{BicyclePrice}, #{CreateTime}, #{PayTime}, #{EffectTime}, #{UnEffectTime}, #{OrderStatus}, #{PayStatus})
    </insert>

</mapper>